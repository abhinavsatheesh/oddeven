CREATE TABLE `livematch` (
    ->   `MatchID` int NOT NULL,
    ->   `UserID1` int DEFAULT NULL,
    ->   `UserID2` int DEFAULT NULL,
    ->   `toss` char(7) DEFAULT NULL,
    ->   `last_num_host` int DEFAULT NULL,
    ->   `last_num_player` int DEFAULT NULL,
    ->   PRIMARY KEY (`MatchID`),
    ->   UNIQUE KEY `UserID1` (`UserID1`),
    ->   UNIQUE KEY `UserID2` (`UserID2`)
    -> );
